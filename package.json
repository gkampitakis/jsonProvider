{
  "name": "jsonprovider",
  "version": "1.3.0",
  "description": "Simple Server for storing and serving json files",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.test.ts",
    "test:env": "NODE_ENV=test ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "prod": "tsc && NODE_ENV=production node ./build/index.js",
    "start": "NODE_ENV=production node ./build/index.js",
    "build": "rimraf ./build && tsc"
  },
  "author": "George Kampitakis",
  "license": "ISC",
  "dependencies": {
    "@gkampitakis/tslog": "file:../logger",
    "async": "^3.1.0",
    "bluebird": "^3.7.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "mongoose": "^5.7.11",
    "nodemailer": "^6.4.2",
    "nodemailer-express-handlebars": "^3.1.0",
    "nodemailer-sendgrid": "^1.0.3",
    "parse-bearer-token": "^1.0.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2",
    "unique-names-generator": "^4.1.1",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "@types/async": "^3.0.3",
    "@types/chai": "^4.2.5",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.12.8",
    "@types/nodemailer": "^6.4.0",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.1.0",
