{
  "name": "jsonprovider",
  "version": "1.0.0",
  "description": "Simple Server for storing and serving json files",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.test.ts",
    "test:env": "NODE_ENV=test ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "prod": "tsc && NODE_ENV=production node ./build/index.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  },
  "author": "George Kampitakis",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.7.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "log4js": "^6.0.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.7.11",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.12.8",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "errorhandler": "^1.5.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.1.0",
    "mocha": "^6.2.2",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
